#: lib/spiderfw/cmd/cmd.rb:19
msgid "Global options:"
msgstr "Opzioni globali:"

#: lib/spiderfw/cmd/cmd.rb:20
msgid "Be verbose when outputting info"
msgstr "Sii prolisso"

#: lib/spiderfw/cmd/cmd.rb:21
msgid "Cd to a directory before running"
msgstr "Spostati in una cartella prima dell'esecuzione"

#: lib/spiderfw/cmd/cmd.rb:22
msgid "Include configuration sets"
msgstr "Includi insiemi di configurazione"

#: lib/spiderfw/cmd/cmd.rb:25
msgid "Set runmode to devel"
msgstr "Imposta il runmode a devel"

#: lib/spiderfw/cmd/commands/cert.rb:8
msgid "Manage certificates"
msgstr "Gestisci certificati"

#: lib/spiderfw/cmd/commands/cert.rb:13
msgid "Generate new X.509"
msgstr "Genera un nuovo X.509"

#: lib/spiderfw/cmd/commands/cert.rb:15
msgid "Where to generate the certificate"
msgstr "Dove generare il certificato"

#: lib/spiderfw/cmd/commands/cert.rb:18
msgid "Name of the organization to generate the certificate for"
msgstr "Nome dell'organizzazione per cui generare il certificato"

#: lib/spiderfw/cmd/commands/console.rb:6
msgid "Open a console"
msgstr "Apri una console"

#: lib/spiderfw/cmd/commands/console.rb:12
msgid "Irb executable to use"
msgstr "Eseguibile irb da usare"

#: lib/spiderfw/cmd/commands/init.rb:8
msgid "Create a working dir for development or installation"
msgstr "Crea una cartella di installazione"

#: lib/spiderfw/cmd/commands/init.rb:12
msgid "Create a new application"
msgstr "Crea un'applicazione vuota"

#: lib/spiderfw/cmd/commands/init.rb:15
msgid ""
"The path where to create the app (defaults to the app folder under the "
"current path)"
msgstr ""
"Il percorso dove creare l'applicazione (di default la cartella app nel "
"percorso corrente)"

#: lib/spiderfw/cmd/commands/init.rb:19
msgid "The module name; by default, the app name will be camelized"
msgstr ""
"Il nome del modulo; di default, il nome dell'applicazione sarà cammellizzato"

#: lib/spiderfw/cmd/commands/init.rb:33
msgid "Create an installation"
msgstr "Crea un'installazione"

#: lib/spiderfw/cmd/commands/init.rb:36
msgid "The path where to create the installation (defaults to the path)"
msgstr ""
"Il percorso dove creare l'installazione (di default il percorso corrente)"

#: lib/spiderfw/cmd/commands/model.rb:6
msgid "Manage models"
msgstr "Gestisci i modelli"

#: lib/spiderfw/cmd/commands/model.rb:11
msgid "Sync models"
msgstr "Sincronizza i modelli"

#: lib/spiderfw/cmd/commands/model.rb:13
msgid "Force syncing"
msgstr "Forza la sincronizzazione"

#: lib/spiderfw/cmd/commands/model.rb:16
msgid "Drop unused columns"
msgstr "Esegui il drop delle colonne inutilizzate"

#: lib/spiderfw/cmd/commands/model.rb:17
msgid "Drop unused tables"
msgstr "Esegui il drop delle tabelle inutilizzate"

#: lib/spiderfw/cmd/commands/model.rb:21
msgid "Update current sequences to max db value"
msgstr "Aggiorna le sequenze esistenti al max in db"

#: lib/spiderfw/cmd/commands/model.rb:51
msgid ""
"The following tables are about to be dropped: \n"
"%s"
msgstr ""
"Le segenti tabelle stanno per essere cancellate: \n"
"%s"

#: lib/spiderfw/cmd/commands/model.rb:52
msgid "Continue? yes/NO: "
msgstr "Continuare? sì/NO"

#: lib/spiderfw/cmd/commands/model.rb:54 lib/spiderfw/cmd/commands/model.rb:57
msgid "yes"
msgstr "sì"

#: lib/spiderfw/cmd/commands/model.rb:55
msgid "no"
msgstr "no"

#: lib/spiderfw/cmd/commands/model.rb:66
msgid "Unable to modify the following fields:"
msgstr "Impossibile modificare i seguenti campi:"

#: lib/spiderfw/cmd/commands/model.rb:68
msgid "(use -f to force)"
msgstr "(usa -f per forzare)"

#: lib/spiderfw/cmd/commands/setup.rb:8
msgid "Setup an application"
msgstr "Esegui il setup di un'applicazione"

#: lib/spiderfw/cmd/commands/setup.rb:15
msgid "Setup to a specific version"
msgstr "Esegui il setup fino a una versione specifica"

#: lib/spiderfw/cmd/commands/test.rb:6
msgid "Manage tests"
msgstr "Gestisci i test"

#: lib/spiderfw/cmd/commands/test.rb:11
msgid "Run tests"
msgstr "Esegui i test"

#: lib/spiderfw/cmd/commands/webserver.rb:8
msgid "Manage internal webserver"
msgstr "Gestisci il webserver interno"

#: lib/spiderfw/cmd/commands/webserver.rb:21
msgid "The port the webserver should listen on"
msgstr "La porta su cui deve stare in ascolto il webserver"

#: lib/spiderfw/cmd/commands/webserver.rb:24
msgid "Which webserver to use; the choices are 'webrick', 'mongrel' and 'thin'"
msgstr "Che webserver usare; le possibilità sono 'webrick', 'mongrel' e 'thin'"

#: lib/spiderfw/cmd/commands/webserver.rb:25
msgid "The webserver %s is not supported"
msgstr "Il webserver %s non è supportato"

#: lib/spiderfw/cmd/commands/webserver.rb:28
msgid "Serve each request spawning a CGI subprocess. Useful in developement."
msgstr ""
"Servi ogni richesta eseguendo un nuovo processo CGI. Utile durante lo "
"sviluppo."

#: lib/spiderfw/cmd/commands/webserver.rb:36
msgid "Using webserver %s"
msgstr "Utilizzo il webserver %s"

#: lib/spiderfw/cmd/commands/webserver.rb:37
msgid "Listening on port %s"
msgstr "In ascolto sulla porta %s"

#: lib/spiderfw/config/configuration.rb:86
msgid "%s is not a configuration option"
msgstr "%s non è un'opzione di configurazione"

#: lib/spiderfw/config/options/spider.rb:12
msgid "The internal server to use"
msgstr "Il server interno da usare"

#: lib/spiderfw/config/options/spider.rb:15
msgid "Whether to show the stack trace on error"
msgstr "Mostrare o no lo stack trace degli errori"

#: lib/spiderfw/config/options/spider.rb:18
msgid "Reload application and spider sources on each request"
msgstr "Ricarica i sorgenti delle applicazioni e di spider a ogni richiesta"

#: lib/spiderfw/config/options/spider.rb:21
msgid "Port to use for the http server"
msgstr "Porta da usare per il server http"

#: lib/spiderfw/config/options/spider.rb:22
msgid "Force threading on non-threaded adapters"
msgstr "Forza l'utilizzo dei thread con adapter non-threaded"

#: lib/spiderfw/config/options/spider.rb:25
msgid "The text editor installed on the client"
msgstr "L'editro di testo installato nel client"

#: lib/spiderfw/config/options/spider.rb:28
msgid "Refresh template cache every time"
msgstr "Aggiorna la cache dei template a ogni richiesta"

#: lib/spiderfw/config/options/spider.rb:29
msgid "Run templates in safe mode"
msgstr "Esegui i template in modalità sicura"

#: lib/spiderfw/config/options/spider.rb:33
msgid "How many connections to open to a db"
msgstr "Quante connessioni al db aprire"

#: lib/spiderfw/config/options/spider.rb:35
msgid "Replace bound variables in debug sql"
msgstr "Sostituisci le bound variables nell'sql di debug"

#: lib/spiderfw/config/options/spider.rb:39
msgid "A list of named storages"
msgstr "Una lista di archivi con nome"

#: lib/spiderfw/config/options/spider.rb:40
msgid "Connection url to the storage"
msgstr "Url di connessione all'archivio"

#: lib/spiderfw/config/options/spider.rb:41
msgid "Encoding the DB uses"
msgstr "La codifica utilizzata dal DB"

#: lib/spiderfw/config/options/spider.rb:43
msgid "Start the debugger"
msgstr "Avvia il debugger"

#: lib/spiderfw/config/options/spider.rb:45
msgid "The locale to use"
msgstr "Il locale da utilizzare"

#: lib/spiderfw/config/options/spider.rb:48
msgid "Path where rails-style locales are found"
msgstr "Percorso dove si trovano il locale in formato rails"

#: lib/spiderfw/config/options/spider.rb:49
msgid "Fallback locale"
msgstr "Locale di default"

#: lib/spiderfw/config/options/spider.rb:51
msgid "Sleep time for the periodic runner"
msgstr "Tempo di sleep per il runner periodico"

#: lib/spiderfw/config/options/spider.rb:53
msgid "Where to store the session"
msgstr "Dove salvare la sessione"

#: lib/spiderfw/config/options/spider.rb:54
msgid "Lifetime in seconds of the sessions"
msgstr "Vita in secondi delle sessioni"

#: lib/spiderfw/config/options/spider.rb:55
msgid "Number of seconds to wait before session purge check"
msgstr "Numero di secondi da aspettare prima di un controllo di pulizia"

#: lib/spiderfw/config/options/spider.rb:56
msgid "The folder where to store file sessions"
msgstr "La cartella dove salvare le sessioni"

#: lib/spiderfw/config/options/spider.rb:58
msgid "Which shared store to use"
msgstr "Che tipo di shared store usare"

#: lib/spiderfw/config/options/spider.rb:60
msgid "Life in seconds of HTTP Digest Authentication nonces"
msgstr "Vita in secondi dei nonce per l'Autenticazione HTTP Digest"

#: lib/spiderfw/config/options/spider.rb:62
msgid "Automatically send headers on first output (breaks the debugger)"
msgstr ""
"Invia automaticamente i header al primo output (interferisce con il debugger)"

#: lib/spiderfw/config/options/spider.rb:63
msgid "Redirect standard output to the browser"
msgstr "Ridirigi lo standard output al browser"

#: lib/spiderfw/config/options/spider.rb:64
msgid ""
"If the request is proxyied, the urls used to reach spider, with the "
"corresponding paths called by the proxy"
msgstr ""
"Se la richiesta è passata attraverso un proxy, gli url usati per raggiungere "
"spider, con i corrispondenti percorsi chiamati dal proxy"

#: lib/spiderfw/config/options/spider.rb:67
msgid "Level of debug output to console"
msgstr "Livello di debug da inviare in console"

#: lib/spiderfw/config/options/spider.rb:69
msgid "Log errors to file"
msgstr "Logga gli errori su file"

#: lib/spiderfw/config/options/spider.rb:70
msgid "Log level to use for debug file (false for no debug)"
msgstr ""
"Livello di log da usare per il file di debug (false per disabilitare il "
"debug)"

#: lib/spiderfw/config/options/spider.rb:76
msgid "A list of organizations"
msgstr "Una lista di organizzazioni"

#: lib/spiderfw/config/options/spider.rb:77
msgid "Descriptive name of the organization"
msgstr "Nome descrittivo dell'organizzazione"

#: lib/spiderfw/config/options/spider.rb:78
msgid "Country code of the organization"
msgstr "Codice nazione dell'organizzazione"

#: lib/spiderfw/config/options/spider.rb:79
msgid "State of the organization"
msgstr "Provincia dell'organizzazione"

#: lib/spiderfw/config/options/spider.rb:80
msgid "Name of the city"
msgstr "Nome della città"

#: lib/spiderfw/config/options/spider.rb:81
msgid "Common name (e.g. domain) of the organization"
msgstr "Common name (p.es. il dominio) dell'organizzazione"

#: lib/spiderfw/config/options/spider.rb:82
msgid "Main e-mail address of the organization"
msgstr "Indirizzo e-mail principale dell'organizzazione"

#: lib/spiderfw/config/options/spider.rb:83
msgid "Organizational Unit (e.g. department)"
msgstr "Unità Organizzativa (p.es. il dipartimento)"

#: lib/spiderfw/config/options/spider.rb:84
msgid "Path to the public key (defaults to config/certs/org_name/public.pem)"
msgstr ""
"Percorso della chiave pubblica (di default config/certs/nome_org/public.pem)"

#: lib/spiderfw/config/options/spider.rb:86
msgid "Path to the certificate (defaults to config/certs/org_name/cert.pem)"
msgstr "Percorso del certificato (di default config/certs/nome_org/cert.pem)"

#: lib/spiderfw/config/options/spider.rb:88
msgid ""
"Path to the private key (defaults to config/certs/org_name/private/key.pem)"
msgstr ""
"Percorso della chiave privata (di default config/certs/nome_org/private/key."
"pem)"

