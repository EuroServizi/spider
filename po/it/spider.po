msgstr "Gestisci i test"

#: lib/spiderfw/cmd/commands/test.rb:11
msgid "Run tests"
msgstr "Esegui i test"

#: lib/spiderfw/cmd/commands/webserver.rb:8
msgid "Manage internal webserver"
msgstr "Gestisci il webserver interno"

#: lib/spiderfw/cmd/commands/webserver.rb:21
msgid "The port the webserver should listen on"
msgstr "La porta su cui deve stare in ascolto il webserver"

#: lib/spiderfw/cmd/commands/webserver.rb:24
msgid "Which webserver to use; the choices are 'webrick', 'mongrel' and 'thin'"
msgstr "Che webserver usare; le possibilità sono 'webrick', 'mongrel' e 'thin'"

