#: lib/spiderfw/cmd/cmd.rb:19
msgid "Global options:"
msgstr "Opzioni globali:"

#: lib/spiderfw/cmd/cmd.rb:20
msgid "Be verbose when outputting info"
msgstr "Sii prolisso"

#: lib/spiderfw/cmd/commands/cert.rb:8
msgid "Manage certificates"
msgstr "Gestisci certificati"

#: lib/spiderfw/cmd/commands/cert.rb:14
msgid "Generate new X.509"
msgstr "Genera un nuovo X.509"

#: lib/spiderfw/cmd/commands/cert.rb:16
msgid "Where to generate the certificate"
msgstr "Dove generare il certificato"

#: lib/spiderfw/cmd/commands/console.rb:6
msgid "Open a console"
msgstr "Apri una console"

#: lib/spiderfw/cmd/commands/console.rb:12
msgid "Irb executable to use"
msgstr "Eseguibile irb da usare"

#: lib/spiderfw/cmd/commands/init.rb:8
msgid "Create a working dir for development or installation"
msgstr "Crea una cartella di installazione"

#: lib/spiderfw/cmd/commands/init.rb:12
msgid "Create a new application"
msgstr "Crea un'applicazione vuota"

#: lib/spiderfw/cmd/commands/init.rb:15
msgid ""
"The path where to create the app (defaults to the app folder under the "
"current path)"
msgstr ""
"Il percorso dove creare l'applicazione (di default la cartella app nel "
"percorso corrente)"

#: lib/spiderfw/cmd/commands/init.rb:19
msgid "The module name; by default, the app name will be camelized"
msgstr ""
"Il nome del modulo; di default, il nome dell'applicazione sarà cammellizzato"

#: lib/spiderfw/cmd/commands/setup.rb:8
msgid "Setup an application"
msgstr "Esegui il setup di un'applicazione"

#: lib/spiderfw/cmd/commands/setup.rb:15
msgid "Setup to a specific version"
msgstr "Esegui il setup fino a una versione specifica"

#: lib/spiderfw/cmd/commands/test.rb:9
msgid "Manage tests"
msgstr "Gestisci i test"

#: lib/spiderfw/cmd/commands/test.rb:14
msgid "Run tests"
msgstr "Esegui i test"

#: lib/spiderfw/cmd/commands/webserver.rb:8
msgid "Manage internal webserver"
msgstr "Gestisci il webserver interno"

#: lib/spiderfw/cmd/commands/webserver.rb:23
msgid "The port the webserver should listen on"
msgstr "La porta su cui deve stare in ascolto il webserver"

#: lib/spiderfw/cmd/commands/webserver.rb:26
msgid "Which webserver to use; the choices are 'webrick' and 'mongrel'"
msgstr "Che webserver usare; le possibilità sono 'webrick' e 'mongrel'"

#: lib/spiderfw/cmd/commands/webserver.rb:27
msgid "The webserver %s is not supported"
msgstr "Il webserver %s non è supportato"

#: lib/spiderfw/cmd/commands/webserver.rb:30
msgid "Serve each request spawning a CGI subprocess. Useful in developement."
msgstr ""
"Servi ogni richesta eseguendo un nuovo processo CGI. Utile durante lo "
"sviluppo."

#: lib/spiderfw/cmd/commands/webserver.rb:35
msgid "Using webserver %s"
msgstr "Utilizzo il webserver %s"

#: lib/spiderfw/cmd/commands/webserver.rb:36
msgid "Listening on port %s"
msgstr "In ascolto sulla porta %s"

#: lib/spiderfw/config/configuration.rb:28
#: lib/spiderfw/config/configuration.rb:43
msgid "%s is not a configuration option"
msgstr "%s non è un'opzione di configurazione"

#: lib/spiderfw/config/options/spider.rb:19
msgid "The internal server to use"
msgstr "Il server interno da usare"

#: lib/spiderfw/config/options/spider.rb:22
msgid "Whether to show the stack trace on error"
msgstr "Mostrare o no lo stack trace degli errori"

#: lib/spiderfw/config/options/spider.rb:25
msgid "Reload application and spider sources on each request"
msgstr "Ricarica i sorgenti delle applicazioni e di spider a ogni richiesta"

#: lib/spiderfw/config/options/spider.rb:29
msgid "The text editor installed on the client"
msgstr "L'editro di testo installato nel client"

#: lib/spiderfw/config/options/spider.rb:32
msgid "Refresh template cache every time"
msgstr "Aggiorna la cache dei template a ogni richiesta"

#: lib/spiderfw/config/options/spider.rb:36
msgid "A list of named storages"
msgstr "Una lista di archivi con nome"

#: lib/spiderfw/config/options/spider.rb:38
msgid "Start the debugger"
msgstr "Avvia il debugger"

#: lib/spiderfw/model/base_model.rb:365
msgid "%s is not in the correct format"
msgstr "%s non è nel formato corretto"

